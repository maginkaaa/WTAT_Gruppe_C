<h2>Jobs Overview-Table</h2>
<table class="table">
 <thead>
 <tr>
<th>Title</th>
<th>Company</th>
<th>Description</th>
<th>Salary</th>
<th>Location</th>
<th>Qualifications</th>
 </tr>
 </thead>
 <tbody>
 <% jobs.forEach(job => { %>
 <tr>
<td><%= job.title %></td>
<td><%= job.company %></td>
<td><%= job.description %></td>
<td><%= job.salary %></td>
<td><%= job.location %></td>
<td><%= job.qualifications %></td>
 </tr>
 <% }); %>
 </tbody>
</table>
